{
  "name": "electron-calendar",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Simple calendar app to reserve and manage appointments",
  "author": "Gennaro Landolfi <code@rinodrummer.dev>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "fullcalendar"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview"
    "preview": "vite preview",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "electron": "^29.3.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-vue-devtools": "^7.1.2",
    "vue": "^3.4.25",
    "vue-tsc": "^2.0.14"
  },
  "dependencies": {
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}
